# K-Means clustering dengan K=3
set.seed(3)
m=matrix(rnorm(900*2), ncol=3)
m[1:300,1]=m[1:300,1]+5
m[1:300,2]=m[1:300,2]-6
m[1:300,3]=m[1:300,3]+7
colnames(m) <- c("x","y","z")
km.out=kmeans(m,3,nstart = 20)
km.out$cluster
plot(m, col=(km.out$cluster+5),
main = "Hasil K-Means Clustering dengan K = 3",
pch=20,
cex=1.5)
points(km.out$centers,
col=4:3,
pch=8,
cex=2)
